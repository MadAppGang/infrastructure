{
  "terraform_version": "1.5.7",
  "resource_changes": [
    {
      "address": "aws_instance.web_server",
      "type": "aws_instance",
      "provider_config_key": "aws",
      "change": {
        "actions": ["delete", "create"],
        "before": {
          "ami": "ami-old123",
          "instance_type": "t2.micro",
          "availability_zone": "us-west-2a",
          "subnet_id": "subnet-12345",
          "tags": {"Name": "WebServer", "Environment": "dev"},
          "security_groups": ["sg-old1", "sg-old2"]
        },
        "after": {
          "ami": "ami-new456",
          "instance_type": "t3.small",
          "availability_zone": "us-west-2b",
          "subnet_id": "subnet-67890",
          "tags": {"Name": "WebServer-v2", "Environment": "dev", "Version": "2.0"},
          "security_groups": ["sg-new1", "sg-new2", "sg-new3"]
        }
      }
    },
    {
      "address": "aws_security_group.api_sg",
      "type": "aws_security_group",
      "provider_config_key": "aws",
      "change": {
        "actions": ["delete"],
        "before": {
          "name": "api-security-group",
          "description": "Security group for API servers",
          "vpc_id": "vpc-12345",
          "ingress": [
            {"from_port": 443, "to_port": 443, "protocol": "tcp", "cidr_blocks": ["10.0.0.0/8"]}
          ],
          "egress": [
            {"from_port": 0, "to_port": 0, "protocol": "-1", "cidr_blocks": ["0.0.0.0/0"]}
          ],
          "tags": {"Name": "API-SG", "Environment": "dev"}
        },
        "after": null
      }
    },
    {
      "address": "aws_rds_instance.database",
      "type": "aws_rds_instance",
      "provider_config_key": "aws",
      "change": {
        "actions": ["update"],
        "before": {
          "engine": "postgres",
          "engine_version": "13.7",
          "instance_class": "db.t3.micro",
          "allocated_storage": 20,
          "storage_encrypted": false,
          "backup_retention_period": 7,
          "multi_az": false
        },
        "after": {
          "engine": "postgres",
          "engine_version": "14.9",
          "instance_class": "db.t3.small",
          "allocated_storage": 50,
          "storage_encrypted": true,
          "backup_retention_period": 30,
          "multi_az": true
        }
      }
    },
    {
      "address": "aws_lambda_function.processor",
      "type": "aws_lambda_function",
      "provider_config_key": "aws",
      "change": {
        "actions": ["create"],
        "before": null,
        "after": {
          "function_name": "data-processor",
          "runtime": "python3.11",
          "handler": "main.lambda_handler",
          "memory_size": 256,
          "timeout": 60,
          "environment": {
            "variables": {
              "STAGE": "dev",
              "API_ENDPOINT": "https://api.example.com"
            }
          },
          "tags": {"Service": "DataProcessing", "Environment": "dev"}
        }
      }
    },
    {
      "address": "aws_s3_bucket.logs",
      "type": "aws_s3_bucket",
      "provider_config_key": "aws",  
      "change": {
        "actions": ["create"],
        "before": null,
        "after": {
          "bucket": "app-logs-dev-2024",
          "acl": "private",
          "versioning": {"enabled": false},
          "lifecycle_rule": [
            {
              "enabled": true,
              "expiration": {"days": 90},
              "transition": [
                {"days": 30, "storage_class": "STANDARD_IA"},
                {"days": 60, "storage_class": "GLACIER"}
              ]
            }
          ],
          "server_side_encryption_configuration": {
            "rule": {
              "apply_server_side_encryption_by_default": {
                "sse_algorithm": "AES256"
              }
            }
          }
        }
      }
    }
  ]
}